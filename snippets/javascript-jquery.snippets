snippet sdownc
	slideDown('${1:slow/400/fast}', function() {
		${2://Stuff to do *after* the animation takes place};
	});
snippet select
	select(function() {
		${1:// Act on the event}
	});
snippet show
	show('${1:slow/400/fast}')${2}
snippet showc
	show('${1:slow/400/fast}', function() {
		${2://Stuff to do *after* the animation takes place}
	});
snippet sib
	siblings('${1:selector}')${2}
snippet size
	size()${1}
snippet stoggle
