snippet one
	one('${1:event name}', function(${2:event}) {
		${3:// Act on the event once}
	});
snippet parent
	parent('${1:selector}')${2}
snippet parents
	parents('${1:selector}')${2}
snippet prepend
	prepend('${1:Some text <b>and bold!</b>}')${2}
snippet prependto
	prependTo('${1:selector}')${2}
snippet prev
	prev('${1:selector}')${2}
snippet ready
