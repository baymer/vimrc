snippet remattr
	removeAttr('${1:attribute name}')${2}
snippet remcl
	removeClass('${1:classname}')${2}
snippet reset
	reset(function() {
		${1:// Act on the event}
	});
snippet resize
	resize(function() {
		${1:// Act on the event}
	});
snippet scroll
	scroll(function() {
		${1:// Act on the event}
	});
snippet sdown
