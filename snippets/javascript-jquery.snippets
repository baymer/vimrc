snippet $
	$(${1:this})${2}
snippet add
	add('${1:selector}')${2}
snippet addcl
	addClass('${1:classname}')${2}
snippet after
	after('${1:Some text <b>and bold!</b>}')${2}
snippet ajax
	$.ajax({
		url: '${1:/url}',
		type: '${2:POST}',
		dataType: '${3:json}',
		data: ${4:{}},

		complete: function() {
			${5:// called when complete}
		},

		success: function() {
			${6:// called when successful}
		},

		error: function() {
			${7:// called when there is an error}
		},
	});
snippet ajaxerror
	.ajaxError(function(${1:request, settings}) {
		${2:// stuff to do when an AJAX call returns an error};
	});
snippet get
	$.get('${1:/url}', function(${2:data}) {
		${4}
	}${3:, 'text'});
snippet post
	$.post('${1:/url}', ${2:params}, function(${3:data}) {
		${5}
	}${4:, 'text'});
snippet getjson
	$.getJSON('${1:/url}', function(${2:data}) {
		${3}
	});
snippet getscr
	$.getScript('${1:somescript.js}', function() {
		${2://optional stuff to do after getScript;}
	});
snippet ajaxgetif
	$.getIfModified('${1:/test/test.cgi}', function(data) {
		${2:alert( "Data loaded: " + data ) //optional stuff to do after get;}
	});
snippet ajaxsend
	.ajaxSend(function(${1:request, settings}) {
		${2://stuff to do when an AJAX call returns an error};
	});
snippet ajaxsetup
	$.ajaxSetup({
		url: "${1:mydomain.com/url}",
		type: "${2:POST}",
		dataType: "${3:xml/html/script/json}",
		data: $.param( $("${4:Element or Expression}") ),

		complete: function() {
			${5://called when complete}
		},

		success: function() {
			${6://called when successful}
		},

		error: function() {
			${7://called when there is an error}
		},
	});
snippet ajaxstart
	$.ajaxStart(function() {
		${1://stuff to do when an AJAX call is started and no other AJAX calls are in progress};
	});
snippet ajaxstop
	$.ajaxStop(function() {
		${1://stuff to do when an AJAX call is started and no other AJAX calls are in progress};
	});
snippet ajaxsuccess
	$.ajaxSuccess(function() {
		${1://stuff to do when an AJAX call is started and no other AJAX calls are in progress};
	});
snippet animate
	animate({${1:param1: value1, param2: value2}}, ${2:speed})${3}
snippet app
	append('${1:Some text <b>and bold!</b>}')${2}
snippet appto
	appendTo(${1:selector})${2}
snippet attr
	attr('${1:attribute}', '${2:value}')${3}
snippet attrm
	attr({'${1:attr1}': '${2:value1}', '${3:attr2}': '${4:value2}'})${5}
snippet before
	before('${1:Some text <b>and bold!</b>}')${2}
snippet bind
	bind('${1:event}', function(${2:event}) {
		${3:// Act on the event}
	});
snippet blur
	blur(function() {
		${1:// Act on the event}
	});
snippet change
	change(function() {
		${1:// Act on the event}
	});
snippet children
	children('${1:selector}')${2}
snippet click
	click(function() {
		${1:// Act on the event}
	})
snippet clone
	clone()${1}
snippet contains
	contains('${1:text to find}')${2}
snippet css
	css('${1:attribute}', '${2:value}')${3}
snippet cssm
	css({${1:attribute1}: '${2:value1}', ${3:attribute2}: '${4:value2}'})${5}
snippet dblclick
	dblclick(function() {
		${1:// Act on the event}
	});
snippet each
	each(function() {
		${1:this.innerHTML = this + " is the element, " + i + " is the position";}
	});
snippet eltrim
	$.trim('${1:string}')${2}
snippet end
	end()${1}
snippet error
	error(function() {
		${1:// Act on the event}
	});
snippet fadein
	fadeIn('${1:slow/400/fast}')${2}
snippet fadeinc
	fadeIn('slow/400/fast', function() {
		${1://Stuff to do *after* the animation takes place};
	});
snippet fadeout
	fadeOut('${1:slow/400/fast}')${2}
snippet fadeoutc
	fadeOut('slow/400/fast', function() {
		${1://Stuff to do *after* the animation takes place};
	});
snippet fadeto
	fadeTo('${1:slow/400/fast}', ${2:0.5})${3}
snippet fadetoc
	fadeTo('slow/400/fast', ${1:0.5}, function() {
		${2://Stuff to do *after* the animation takes place};
	});
snippet filter
	filter('${1:selector}')${2}
snippet find
	find('${1:selector}')${2}
snippet focus
	focus(function() {
		${1:// Act on the event}
	});
snippet height
	height(${1:integer})${2}
snippet hide
	hide('${1:slow/400/fast}')${2}
snippet hidec
	hide('${1:slow/400/fast}', function() {
		${2://Stuff to do *after* the animation takes place}
	});
snippet hover
	hover(function() {
		${1:// Stuff to do when the mouse enters the element;}
	}, function() {
		${2:// Stuff to do when the mouse leaves the element;}
	});
snippet html
	html('${1:Some text <b>and bold!</b>}')${2}
snippet insertAfter
	insertAfter('${1:selector}')${2}
snippet insertBefore
	insertBefore('${1:selector}')${2}
snippet is
	is('${1:selector}')${2}
snippet load
	load(function() {
		${1:// Act on the event}
	});
snippet loadf
	<+obj+>.load('<+/path/to/file.htm+>', { <+<+param1+>: "<+value1+>", <+param2+>: "<+value2+>"+> }, function() {
		<+// Stuff to do after the page is loaded+>
	});
snippet loadif
	<+obj+>.loadIfModified('<+/path/to/file.htm+>', { <+<+param1+>: "<+value1+>", <+param2+>: "<+value2+>"+> }, function() {
		<+// Stuff to do after the page is loaded+>
	});
snippet mdown
	mousedown(function() {
		${1:// Act on the event}
	});
snippet mmove
	mousemove(function() {
		${1:// Act on the event}
	});
snippet mout
	mouseout(function() {
		${1:// Act on the event}
	});
snippet mover
	mouseover(function() {
		${1:// Act on the event}
	});
snippet mup
	mouseup(function() {
		${1:// Act on the event}
	});
snippet next
	next('${1:selector}')${2}
snippet not
