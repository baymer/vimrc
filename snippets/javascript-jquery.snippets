snippet submit
	submit(function() {
		${1:// Act on the event once}
	});
snippet sup
	slideUp('${1:slow/400/fast}')${2}
snippet supc
	slideUp('${1:slow/400/fast}', function() {
		${2://Stuff to do *after* the animation takes place};
	});
snippet text
	text(${1:'some text'})${2}
snippet tog
	toggle(function() {
		${1:// Stuff to do every *odd* time the element is clicked;}
	}, function() {
		${2:// Stuff to do every *even* time the element is clicked;}
	});
snippet togcl
	toggleClass('${1:classname}')${2}
snippet togsh
	toggle('${1:slow/400/fast}')${2}
snippet trig
